{
  "name": "@edx/frontend-app-discussions",
  "version": "0.1.0",
  "description": "Discussions Frontend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openedx/frontend-app-discussions.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --ext .js --ext .jsx . --fix",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/openedx/frontend-app-discussions#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/openedx/frontend-app-discussions/issues"
  },
  "dependencies": {
    "@edx/frontend-platform": "^5.0.0",
    "@edx/paragon": "21.5.6",
    "@reduxjs/toolkit": "1.8.1",
    "@tinymce/tinymce-react": "3.14.0",
    "babel-polyfill": "6.26.0",
    "classnames": "2.3.2",
    "core-js": "3.33.0",
    "dompurify": "^2.4.3",
    "formik": "2.2.9",
    "lodash.snakecase": "4.1.1",
    "prop-types": "15.8.1",
    "raw-loader": "4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
    "react-router": "6.16.0",
    "react-router-dom": "6.18.0",
    "redux": "4.2.1",
    "regenerator-runtime": "^0.14.0",
    "timeago.js": "4.0.2",
    "tinymce": "5.10.7",
    "yup": "0.31.1"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.2.0",
    "@edx/frontend-build": "13.0.5",
    "@edx/reactifex": "2.2.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.0.0",
    "axios-mock-adapter": "1.22.0",
    "babel-plugin-react-intl": "8.2.25",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "glob": "10.3.10",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "rosie": "2.1.0"
  }
}
